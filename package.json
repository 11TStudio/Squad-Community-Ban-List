{
  "name": "Squad-Community-Ban-List",
  "version": "0.0.1",
  "repository": "https://github.com/Squad-Community-Ban-List/Squad-Community-Ban-List.git",
  "author": "Squad Community Ban List Team <https://github.com/Squad-Community-Ban-List>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "assets",
    "core",
    "database",
    "ban-exporter",
    "ban-importer",
    "web-server",
    "client"
  ],
  "scripts": {
    "lint": "eslint --fix . && prettier --write \"./**/*.js\"",
    "start-ban-exporter": "node ban-exporter/index.js",
    "dev-ban-exporter": "nodemon ban-exporter/index.js",
    "start-ban-importer": "node ban-importer/index.js",
    "dev-ban-importer": "nodemon ban-importer/index.js",
    "dev-web-server": "concurrently \"nodemon web-server/index.js\" \"cd client && npm run start\""
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1"
  }
}
